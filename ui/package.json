{
  "name": "ivpn-ui",
  "version": "3.14.32",
  "productName": "IVPN",
  "description": "IVPN Client",
  "author": "IVPN Limited",
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ivpn/desktop-app/"
  },
  "scripts": {
    "init": "node pre_build.js",

    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run init && electron-vite build",

    "electron:build": "npm run build && electron-builder --config electron-builder.config.js",
    
    "electron:build:win": "npm run build && electron-builder --win --config",
    "electron:build:mac": "npm run build && electron-builder --mac --config",
    "electron:build:linux": "npm run build && electron-builder --linux --config",
    
    "postinstall": "electron-builder install-app-deps",
    
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "main": "./out/main/background.js",
  "dependencies": {
    "@sentry/electron": "4.11.1",
    "auto-launch": "5.0.6",
    "click-outside-vue3": "4.0.1",
    "electron-log": "4.4.8",
    "pbkdf2": "3.1.3",
    "qrcode-generator": "1.4.4",
    "winreg": "1.2.5"
  },
  "optionalDependencies": {
    "wifi-info-macos": "file:addons/wifi-info-macos"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "1.0.2",
    "@rushstack/eslint-patch": "1.11.0",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/eslint-config-prettier": "8.0.0",
    "electron": "25.9.8",
    "electron-builder": "24.13.3",
    "electron-vite": "2.3.0",
    "eslint": "8.57.1",
    "eslint-plugin-vue": "9.33.0",
    "prettier": "3.5.3",
    "sass": "1.78.0",
    "vite": "5.4.16",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vuex": "4.1.0",
    "vuex-electron": "1.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
